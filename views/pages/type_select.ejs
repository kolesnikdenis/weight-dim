<section class="type-select">
	<div class="container">
    <div class="row">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a class="block_link" href="/">Главная</a></li>
          <li class="breadcrumb-item"><a class="block_link header_text" href="/sel_products?type=<%- type_operation %>"></a></li>
          <li class="breadcrumb-item active" aria-current="page"><%- name %></a></li>  
        </ol>
      </nav>
    </div>
	    <div class="row">		
	        <div class="col-12 offset-md-2 col-md-8">
			<button type="button" class="btn btn-choose" id=chicken onclick="select('chicken')">Курица</button>
		</div>
	        <div class="col-12 offset-md-2 col-md-8">
			<!-- <div style="width:100%"  class="btn-group-vertical btn-group-lg"> -->
			<button type="button" class="btn btn-choose" id=turkey onclick="select('turkey')">Индюшка</button>
		</div>
		<div class="col-12 offset-md-2 col-md-8">
			<button type="button" class="btn btn-choose" id=pork onclick="select('pork')">Свинина</button>
		</div>
	    </div>
	</div>
</section>

